{% extends 'base.html.twig' %}

{% block title %}Area{% endblock %}

{% block body %}
    <h1>Area</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ area.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ area.name }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ area.slug }}</td>
            </tr>
            <tr>
                <th>Orientation</th>
                <td>{{ area.orientation }}</td>
            </tr>
            <tr>
                <th>Sequence</th>
                <td>{{ area.sequence }}</td>
            </tr>
            <tr>
                <th>Online</th>
                <td>{{ area.online }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ area.image }}</td>
            </tr>
            <tr>
                <th>HeaderImage</th>
                <td>{{ area.headerImage }}</td>
            </tr>
            <tr>
                <th>Lat</th>
                <td>{{ area.lat }}</td>
            </tr>
            <tr>
                <th>Lng</th>
                <td>{{ area.lng }}</td>
            </tr>
            <tr>
                <th>Zoom</th>
                <td>{{ area.zoom }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_area_index') }}">back to list</a>

    <a href="{{ path('app_area_edit', {'id': area.id}) }}">edit</a>

    {{ include('area/_delete_form.html.twig') }}
